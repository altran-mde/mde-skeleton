<settings xmlns="http://maven.apache.org/SETTINGS/1.0.0"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.0.0
                      http://maven.apache.org/xsd/settings-1.0.0.xsd">
  <servers>
    <server>
      <id>acidspace-maven-central</id>
      <configuration>
        <timeout>10000</timeout>
      </configuration>
      <username>${env.ArtifactStoreUserName}</username>
      <password>${env.ArtifactStorePassword}</password>
    </server>
  </servers>
  <mirrors>
    <mirror>
      <id>acidspace-maven-central</id>
      <name>ACIDSpace Maven Central</name>
      <url>https://artifactory.acidspace.nl/artifactory/maven-central/</url>
      <mirrorOf>central</mirrorOf>
    </mirror>
  </mirrors>
</settings>